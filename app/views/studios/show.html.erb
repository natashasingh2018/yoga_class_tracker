

<div class="page-header">
  <h3>Studio "<%= @studio.name %>"</h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @studio.name %></dd>

      <dt>Location</dt>
      <dd><%= @studio.location %></dd>
      <dt>Latitude</dt>
      <dd><%= @latitude %></dd>
      <dt>Longitude</dt>
      <dd><%= @longitude %></dd>


      <dt>Map</dt>
<dd>
insert map here
</dd>


    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/studios" class="btn btn-primary">
        Back
      </a>
      <a href="/studios/<%= @studio.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_studio/<%= @studio.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

<div id="map"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: <%= @latitude %>, lng: <%= @longitude %>},
      zoom: 8
    });
  }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD6Q8T-Q51aSFadFfohlbcdHG8R9z2pwcc&callback=initMap"
async defer></script>
